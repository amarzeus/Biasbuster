{
  "name": "biasbuster",
  "version": "1.0.0",
  "description": "AI Bias Detection and Mitigation Dashboard",
  "main": "dist/index.js",
  "scripts": {
    "start": "node start-server.js",
    "start:prod": "NODE_ENV=production node start-server.js",
    "dev": "NODE_ENV=development node start-server.js",
    "build": "tsc",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "test": "jest",
    "test:e2e": "jest __tests__/web-platform.test.js",
    "test:unit": "jest __tests__/script.test.js",
    "test:api": "jest __tests__/api.test.js",
    "test:performance": "jest __tests__/performance.test.js",
    "test:security": "jest __tests__/security.test.js",
    "test:verify": "node scripts/verify-tests.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='jest-junit'"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.17.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.1",
    "jspdf": "^3.0.1",
    "lodash-es": "^4.17.21",
    "mongoose": "^7.4.0",
    "react-chartjs-2": "^5.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-validator": "^2.20.33",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.3.23",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autocannon": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.10",
    "puppeteer": "^24.9.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.8.3"
  }
